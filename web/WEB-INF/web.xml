<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Ejemplo</servlet-name>
        <servlet-class>controlador.profesores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>estudiante</servlet-name>
        <servlet-class>controlador.estudiante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaEstudiante</servlet-name>
        <servlet-class>controlador.BusquedaEstudiante</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>BusquedaProfesores</servlet-name>
        <servlet-class>controlador.BusquedaProfesores</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>BusquedaCursos</servlet-name>
        <servlet-class>controlador.BusquedaCursos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inscripciones</servlet-name>
        <servlet-class>controlador.inscripciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CursosDao</servlet-name>
        <servlet-class>controlador.cursos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ejemplo</servlet-name>
        <url-pattern>/Ingresodocentes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>estudiante</servlet-name>
        <url-pattern>/Ingreso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaEstudiante</servlet-name>
        <url-pattern>/BuscarAlumno</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>BusquedaProfesores</servlet-name>
        <url-pattern>/BuscarProfesor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaCursos</servlet-name>
        <url-pattern>/BuscarCurso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>inscripciones</servlet-name>
        <url-pattern>/Inscripciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CursosDao</servlet-name>
        <url-pattern>/IngresoC</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
